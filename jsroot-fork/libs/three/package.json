{
  "name": "three_jsroot",
  "version": "0.153.0",
  "type": "module",
  "description": "JavaScript 3D library",
  "main": "Three_jsroot.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mrdoob/three.js"
  },
  "sideEffects": false,
  "files": [
    "Three_jsroot.js",
    "package.json",
    "README.md"
  ],
  "eslintConfig": {
    "root": true,
    "extends": "mdcs",
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "babelOptions": {
        "configFile": "./.babelrc.json"
      }
    },
    "plugins": [
      "html"
    ],
    "globals": {
      "potpack": true,
      "fflate": true,
      "ZSTDDecoder": true,
      "bodymovin": true,
      "OIMO": true,
      "Stats": true,
      "XRWebGLBinding": true,
      "XRWebGLLayer": true
    },
    "rules": {
      "quotes": [
        "error",
        "single"
      ],
      "prefer-const": [
        "error",
        {
          "destructuring": "any",
          "ignoreReadBeforeAssign": false
        }
      ]
    }
  },
  "scripts": {
    "build": "patch ../../../threejs/examples/jsm/controls/OrbitControls.js OrbitControls.patch && cp Three_jsroot.js ../../../threejs/src && rollup -c rollup.jsroot.config.js && rm ../../../threejs/src/Three_jsroot.js && git -C ../../../threejs restore examples/jsm/controls/OrbitControls.js && rm -f ../../../threejs/examples/jsm/controls/OrbitControls.js.orig"
  },
  "keywords": [
    "three",
    "three.js",
    "javascript",
    "3d",
    "virtual-reality",
    "augmented-reality",
    "webgl",
    "webgl2",
    "webaudio",
    "webgpu",
    "webxr",
    "canvas",
    "svg",
    "html5"
  ],
  "author": "mrdoob",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mrdoob/three.js/issues"
  },
  "homepage": "https://threejs.org/",
  "devDependencies": {
    "@rollup/plugin-json": "6",
    "@rollup/plugin-terser": "0",
    "rollup": "3"
  }
}
